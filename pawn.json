{
	"user": "AliLogic",
	"repo": "ugmp-stdlib",
	"include_path": "includes",
	"resources": [
		{
			"name": "^ugmp-compat-(.*)-linux.tar.gz$",
			"platform": "linux",
			"archive": true,
			"plugins": ["ugmp.so"]
		},
		{
			"name": "^ugmp-compat-(.*)-win32.zip$",
			"platform": "windows",
			"archive": true,
			"plugins": ["ugmp.dll"]
		}
	],
	"runtime": {
		"plugins": [
			"AliLogic/ugmp-stdlib"
		]
	},
	"dependencies": [
		"pawn-lang/sa-mp-fixes",
		"samp-plugin-crashdetect:v4.20"
	],
	"contributors": [
		"alilogic",
		"alextwothousand"
	]
}
